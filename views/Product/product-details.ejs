		<div class="page-wrapper">
			<div class="content">
				<div class="page-header">
					<div class="page-title">
						<h4>Product Details</h4>
						<h6>Full details of a product</h6>
					</div>
				</div>
				<!-- /add -->
				<div class="row">
					<div class="col-lg-8 col-sm-12">
						<div class="card">
							<div class="card-body">
								<div class="productdetails">
									<ul class="product-bar">
										<li>
											<h4>Product</h4>
											<h6><%= product.product %>	</h6>
										</li>
										<li>
											<h4>Category</h4>
											<h6><%= product.category.category_name %></h6>
										</li>
										<li>
											<h4>Unit</h4>
											<select id="unitSelect" class="form-select">
												<% product.variants.forEach(variant => { %>
												<option value="<%= variant.unitCode %>"
													data-price="<%= variant.sellPrice %>"
													data-qty="<%= variant.quantity %>">
													<%= variant.unitCode %>
												</option>
												<% }) %>
											</select>
											</li>
											<li>
											<h4>Quantity</h4>
											<h6 id="productQty"><%= product.variants[0] ? product.variants[0].quantity : '-' %></h6>
											</li>
											<li>
											<h4>Sell Price</h4>
											<h6 id="productSellPrice">₦<%= product.variants[0] ? product.variants[0].sellPrice : '-' %></h6>
										</li>
										<script>
										document.addEventListener("DOMContentLoaded", function() {
											const unitSelect = document.getElementById("unitSelect");
											const qtyElement = document.getElementById("productQty");
											const priceElement = document.getElementById("productSellPrice");

											unitSelect.addEventListener("change", function() {
											const selectedOption = unitSelect.options[unitSelect.selectedIndex];
											const price = selectedOption.getAttribute("data-price");
											const qty = selectedOption.getAttribute("data-qty");

											qtyElement.textContent = qty;
											priceElement.textContent = "₦" + price;
											});
										});
										</script>

										<li>
											<h4>mfgDate</h4>
											<h6><%= product.mfgDate ? product.mfgDate.toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) : '-' %></h6>
										</li>
										<li>
											<h4>expDate</h4>
											<h6><%= product.expDate ? product.expDate.toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) : '-' %></h6>
										</li>
										<li>
											<h4>Supplier Price</h4>
											<h6><%= product.supplierPrice %></h6>
										</li>
										<li>
											<h4>Created At</h4>
											<h6><%= product.created_at ? product.created_at.toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) : '-' %></h6>
										</li>
										<li>
											<h4>Description</h4>
											<h6><%= product.product_detail %></h6>
										</li>
									</ul>
								</div>
							</div>
						</div>
					</div>
					<div class="col-lg-4 col-sm-12">
						<div class="card">
							<div class="card-body">
								<div class="slider-product-details">
									<div class="owl-carousel owl-theme product-slide">
										<div class="slider-product">
											<img src="/media/uploads/<%= product.product_image %>" alt="img">
											<h4><%= product.product %> - <%= product.category.category_name %></h4>
										</div>
										<div class="slider-product">
											<img src="/media/uploads/<%= product.product_image %>" alt="img">
											<h4><%= product.product %> - <%= product.category.category_name %></h4>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
					
				<!-- /add -->
			</div>
