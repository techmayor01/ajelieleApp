                                                        <button type="button" class="deleteRowBtn" style="background-color: transparent; color: red; border: none;">
                                                            <i class="fa fa-trash"></i>
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
