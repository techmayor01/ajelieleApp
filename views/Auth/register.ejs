<body class="account-page bg-white">

    <div id="global-loader" >
        <div class="whirly-loader"> </div>
    </div>

    <!-- Main Wrapper -->
    <div class="main-wrapper">
        <div class="account-content">
            <div class="login-wrapper login-new">
                <div class="row w-100">
                    <div class="col-lg-5 mx-auto">
                        <div class="login-content user-login">
                            <div class="login-logo1">
                                <h1 class="mb-3">AJALI AND SONS</h1>
                                <a href="index.html" class="login-logo logo-white">
                                    <img src="assets/img/logo-white.svg"  alt="Img">
                                </a>
                            </div>
                            <form action="/register" method="POST">
                                <div class="card">
                                    <div class="card-body p-5">
                                        <div class="login-userheading">
                                            <h3>Register</h3>
                                            <h4>Create New Account To Claim Ownership</h4>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Name <span class="text-danger"> *</span></label>
                                            <div class="input-group">
                                                <input type="text" name="fullname" value="" class="form-control border-end-0">
                                                <span class="input-group-text border-start-0">
                                                    <i class="ti ti-user"></i>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Email <span class="text-danger"> *</span></label>
                                            <div class="input-group">
                                                <input type="text" name="username" autocomplete="off" value="" class="form-control border-end-0">
                                                <span class="input-group-text border-start-0">
                                                    <i class="ti ti-mail"></i>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Branch Name <span class="text-danger"> *</span></label>
                                            <div class="input-group">
                                                <input type="text" name="branch_name" value="" class="form-control border-end-0">
                                                <span class="input-group-text border-start-0">
                                                   <i class="fa-regular fa-address-card" style="font-size: 13px;"></i>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Branch Address <span class="text-danger"> *</span></label>
                                            <div class="input-group">
                                                <input type="text" name="branch_address" value="" class="form-control border-end-0">
                                                <span class="input-group-text border-start-0">
                                                    <i class="fa-regular fa-map" style="font-size: 13px;"></i>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Branch Contact <span class="text-danger"> *</span></label>
                                            <div class="input-group">
                                                <input type="text" name="branch_phone" class="form-control border-end-0">
                                                <span class="input-group-text border-start-0">
                                                    <i class="fa-regular fa-address-book" style="font-size: 13px;"></i>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label">Password <span class="text-danger"> *</span></label>
                                            <div class="pass-group">
                                                <input type="password" name="password" class="pass-input form-control">
                                                <span class="ti toggle-password ti-eye-off text-gray-9"></span>
                                            </div>
                                        </div>
                                        <input type="hidden" value="owner" name="role" id="">
                                        <div class="form-login">
                                            <button type="submit" class="btn btn-login">Sign Up</button>
                                        </div>
                                        <div class="signinform">
                                            <h4>Already have an account ? <a href="/" class="hover-a">Sign In Instead</a></h4>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <div class="my-4 d-flex justify-content-center align-items-center copyright-text">
                            <p>Copyright &copy; 2025 Ajali</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
